<h1>View Currently Checked-Out Movies</h1>

<table>
<thead>
    <tr>
    <th>Patron_ID</th>
    <th>Movie_ID</th>
    <th>Patron First Name</th>
    <th>Patron Last Name</th>
    <th>Movie Title</th>
  </tr>
        </thead>
    <tbody>
      {{#each movies}}
      <tr>
      <td>{{Patron_ID}}</td>
      <td>{{Movie_ID}}</td>
      <td>{{First_name}}</td>
      <td>{{Last_name}}</td>
      <td>{{Title}}</td>
      <td><button onclick="returnMovie({{Patron_ID}}, {{Movie_ID}})">Return</button></td>
    </tr>
      {{/each}}
    </tbody>
</table>
